{
  "name": "product-docs",
  "description": "Developer resources and documentation for the emnify SuperNetwork.",
  "version": "1.0.0",
  "license": "MIT",
  "author": "emnify Documentation Team <docs@emnify.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/emnify/product-docs.git"
  },
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "yarn eslint && yarn prettier",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "markdownlint": "markdownlint \"./**/*.md*\"",
    "markdownlint:fix": "markdownlint \"./**/*.md*\" --fix",
    "eslint": "eslint \"./**/*.js*\"",
    "eslint:fix": "eslint \"./**/*.js*\" --fix",
    "prettier": "prettier \"./**/*.*\" --check",
    "prettier:fix": "prettier \"./**/*.*\" --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "^3.0.1",
    "@docusaurus/plugin-client-redirects": "^3.0.1",
    "@docusaurus/preset-classic": "^3.0.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^1.2.1",
    "docusaurus-plugin-image-zoom": "^0.1.1",
    "dotenv": "^16.2.0",
    "prism-react-renderer": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.0.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.32.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "markdownlint": "^0.31.1",
    "prettier": "^2.8.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "lint-staged": {
    "./**/*.js*": [
      "yarn lint:fix"
    ],
    "./**/*.md*": [
      "yarn markdownlint:fix",
      "vale --config=./.vale.ini --minAlertLevel=error"
    ]
  }
}
